#include <stdio.h>
#include <stdlib.h>
#include <conio.h>

int main()
{
    printf("\n\n\t\t\tCAFUFA DUPLO\n");

    int numeros, primeiro, segundo;

    printf("\nInsira a quantidade de numeros que serao contados no final:");
    scanf("%d", &numeros);
    printf("\nPrimeiro numero:");
    scanf("\n%d", &primeiro);
    printf("\nSegundo numero:");
    scanf("\n%d", &segundo);

    int cont = 0;

    for (int j = 1; j < numeros + 1; j++)
    {
        int a, b, c, d;

        cont = cont + 1;

        a = j % primeiro;
        b = j % segundo;
        c = (j - primeiro) % 10;
        d = (j - segundo) % 10;

        if ((a == 0) || (b == 0) || (c == 0) || (d == 0))
        {
            printf("Cafufa, ");
        }
        else
        {
            printf("%i", j);
            // printf(", ");
        }
    }

    return 0;
}
